<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ConnexionUtilisateur</servlet-name>
		<servlet-class>controleur.ConnexionUtilisateur</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InscriptionUtilisateur</servlet-name>
		<servlet-class>controleur.InscriptionUtilisateur</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>test</servlet-name>
		<servlet-class>controleur.InscriptionUtilisateur</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ConnexionUtilisateur</servlet-name>
		<url-pattern>/connexion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InscriptionUtilisateur</servlet-name>
		<url-pattern>/Album_Photo/Projet/WebContent/www/album_listing.jsp
		</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>test</servlet-name>
		<url-pattern>/connexion_test.jsp</url-pattern>
	</servlet-mapping>
</web-app>